  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "whatwg-fetch": "^2.0.3"
  },
  "prepush": [
    "npm run lint --silent"
  ]
}
