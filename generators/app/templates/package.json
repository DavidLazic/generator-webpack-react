{
  "name": "react-redux-boilerplate",
  "version": "00.00.00",
  "description": "React/Redux boilerplate",
  "author": "",
  "scripts": {
    "clean": "rimraf dist",
    "prod": "npm run clean && cross-env NODE_ENV=production webpack",
    "qa": "npm run clean && cross-env NODE_ENV=qa webpack",
    "dev": "npm run clean && cross-env NODE_ENV=development webpack --display-error-details",
    "local": "npm run clean && cross-env NODE_ENV=local npm start",
    "start": "webpack-dev-server",
    "lint": "eslint src -f table --ext .js --ext .jsx",
    "precommit": "npm run lint"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.6.1",
    "compression-webpack-plugin": "^1.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^1.1.3",
    "html-webpack-plugin": "^2.24.1",
    "husky": "^0.14.3",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "preload-webpack-plugin": "^2.0.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.4.3",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.1",
    "style-ext-html-webpack-plugin": "^3.4.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
