{
  "name": "react-redux-boilerplate",
  "version": "00.00.00",
  "description": "React/Redux boilerplate",
  "author": "",
  "scripts": {
    "clean": "rimraf dist",
    "prod": "npm run clean && cross-env NODE_ENV=production webpack --config=configs/webpack/prod.js",
    "qa": "npm run clean && cross-env NODE_ENV=staging webpack --config=configs/webpack/qa.js ",
    "dev": "npm run clean && cross-env NODE_ENV=development webpack --config=configs/webpack/dev.js",
    "local": "cross-env NODE_ENV=local webpack-dev-server --config configs/webpack/local --progress",
    "start": "npm run local",
    "lint": "eslint src -f table --ext .js --ext .jsx",
    "precommit": "npm run lint"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.5",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@material-ui/core": "^1.4.3",
    "@material-ui/icons": "^2.0.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.5.4",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.11",
    "eslint": "4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.3",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "node-sass": "^4.9.3",
    "path": "^0.12.7",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "react-hot-loader": "^4.3.6",
    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.7.10",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-logger": "^3.0.6",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "svg-url-loader": "^2.3.2",
    "url-loader": "^0.5.7",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-merge": "^4.1.4",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.3.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  },
  "browserslist": {
    "production": [
      "last 2 version",
      "ie 11"
    ],
    "development": [
      "last 1 version"
    ]
  }
